<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="60dp">



    <Row>
        <Key android:keyEdgeFlags="left" android:codes="91" android:keyLabel="[" />
        <Key android:codes="93" android:keyLabel="]"/>
        <Key android:codes="123" android:keyLabel="{"/>
        <Key android:codes="125" android:keyLabel="}"/>
        <Key android:codes="35" android:keyLabel="#"/>
        <Key android:codes="37" android:keyLabel="%"/>
        <Key android:codes="94" android:keyLabel="^"/>
        <Key android:codes="42" android:keyLabel='*'/>
        <Key android:codes="43" android:keyLabel="+"/>
        <Key android:codes="61" android:keyLabel='=' android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="45" android:keyLabel="-" android:keyEdgeFlags="left"/>
        <Key android:codes="92" android:keyLabel="\\"/>
        <Key android:codes="124" android:keyLabel="|"/>
        <Key android:codes="126" android:keyLabel="~"/>


        <Key android:codes="60" android:keyLabel="\u003C"/>
        <Key android:codes="62" android:keyLabel=">"/>
        <Key android:codes="960" android:keyLabel="\u03C0"/>
        <Key android:codes="952" android:keyLabel="\u03B8"/>
        <Key android:codes="955" android:keyLabel="\u03BB"/>
        <Key android:codes="946" android:keyLabel="\u03B2" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:codes="-2" android:keyLabel="123" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="46" android:keyLabel="." android:keyWidth="15%p"/>
        <Key android:codes="44" android:keyLabel="," android:keyWidth="15%p"/>
        <Key android:codes="3F" android:keyLabel="?" android:keyWidth="15%p"/>
        <Key android:codes="21" android:keyLabel="!" android:keyWidth="15%p"/>
        <Key android:codes="27" android:keyLabel="'" android:keyWidth="15%p"/>
        <Key android:codes="-5" android:keyLabel="DEL" android:keyEdgeFlags="right"/>

    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-6" android:keyLabel="ABC" android:keyWidth="10%p"  android:keyEdgeFlags="left"/>
        <Key android:codes="32" android:keyLabel="SPACE" android:keyWidth="70%p" android:isRepeatable="true"/>

        <Key android:codes="-4" android:keyLabel="DONE" android:keyEdgeFlags="right" android:keyWidth="20%p"/>
    </Row>


</Keyboard>